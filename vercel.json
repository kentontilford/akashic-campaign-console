{
  "framework": "nextjs",
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "SKIP_REDIS": "true",
    "DISABLE_RATE_LIMITING": "true"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "1",
      "NODE_OPTIONS": "--max-old-space-size=4096",
      "SKIP_REDIS": "true",
      "DISABLE_RATE_LIMITING": "true"
    }
  },
  "regions": ["iad1"],
  "trailingSlash": false
}