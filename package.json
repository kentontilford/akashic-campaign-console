{
  "name": "akashic-intelligence",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node --max-old-space-size=4096 ./node_modules/.bin/next build",
    "build:railway": "node scripts/build-with-memory.js",
    "build:minimal": "node --max-old-space-size=4096 scripts/build-minimal.js",
    "start": "node server.js",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy || prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "check-env": "node scripts/check-env.js",
    "prebuild": "npm run prisma:generate",
    "build:production": "npm run prisma:generate && next build",
    "db:backup": "node scripts/db-backup.js",
    "db:restore": "node scripts/db-restore.js",
    "postinstall": "prisma generate"
  },
